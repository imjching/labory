<div class="subhead">
  <%= link_to 'Settings', edit_stafftools_course_path(@course), class: 'right btn btn-sm' %>
  <h2 class="subhead-heading"><%= @course.title %></h2>
</div>
<p class="dashboard-notice">
  <%= @course.description %>
</p>

<div class="table-list-header table-list-bordered">
  <%= link_to new_stafftools_course_lab_path(@course), class: 'table-list-action btn btn-sm btn-primary', role: 'button' do %>
    <%= octicon('plus') %>&nbsp;&nbsp;New lab
  <% end %>
  <div class="table-list-header-toggle left">
    <h4>Labs</h4>
  </div>
</div>

<ul class="table-list table-list-bordered application-authorizations" id="sortable">
<% @labs.each do |lab| %>
  <li id="app-5adb6a29c586908f8161" data-sort-url=<%= sort_stafftools_course_lab_path(@course, lab.id) %> class="table-list-item">
    <div id="oauth-authorization-5adb6a29c586908f8161" class="js-revoke-item" data-id="5adb6a29c586908f8161" data-type="application">

      <div class="table-list-cell oauth-logo-cell handle">
        ::::::::
      </div>

      <div class="table-list-cell oauth-info-cell">
        <%= link_to lab.title, stafftools_course_lab_path(@course, lab), class: 'oauth-app-access-name' %>
        <p class="oauth-app-list-meta">
          Last updated in <%= time_ago_in_words(lab.updated_at) %> · Created by <a href="/airpair" class="oauth-app-owner">imjching</a>
        </p>
      </div>

      <div class="table-list-cell oauth-view-revoke-cell">
        <a href="#revoke_authorization_5adb6a29c586908f8161" class="btn btn-sm btn-danger" rel="facebox">Delete</a>
        <div id="revoke_authorization_5adb6a29c586908f8161" style="display:none">
          <div>
            <h2 class="facebox-header" data-facebox-id="facebox-header">Are you sure you want to revoke authorization?</h2>

            <div class="facebox-alert" data-facebox-id="facebox-description">

              airpaircom will no longer be able to access the GitHub API. You cannot undo this action.
            </div>


           <form accept-charset="UTF-8" action="/settings/connections/applications/5adb6a29c586908f8161" class="js-revoke-access-form" data-form-nonce="bc2b189e4d8d0056338f57547fc0ca88880cb8f5" data-id="5adb6a29c586908f8161" data-remote="true" data-type-name="application" method="post"><div style="margin:0;padding:0;display:inline"><input name="utf8" type="hidden" value="✓"><input name="_method" type="hidden" value="delete"><input name="authenticity_token" type="hidden" value="QFU1XHbICC+jM5Ew9Wwh5pq28eW2m0dy81FUdBBY8/zsHtdGuLBUqVrGHIqAPpx0MVNu7ZnZZ7kktuJt/xwLbg=="></div>
              <button type="submit" class="btn btn-block btn-danger" data-disable-with="Revoking…">
                I understand, revoke access
              </button>
</form>          </div>
        </div>
      </div>


    </div>
  </li>
<% end %>














  </ul>
